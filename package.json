{
    "name": "rxjs-dew-history",
    "version": "0.7.0",
    "description": "History bindings for rxjs-dew",
    "main": "js/dist/index.js",
    "typings": "js/dist/index",
    "readme": "README.md",
    "scripts": {
        "clean_cjs": "shx rm -rf js/dist",
        "generate_package": "node .make-package.js",
        "test": "npm-run-all clean_cjs compile_cjs generate_package",
        "compile_cjs": "tsc",
        "build": "npm run compile_cjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elhedran/rxjs-dew-history.git"
    },
    "keywords": [
        "typescript",
        "rxjs",
        "history"
    ],
    "author": "elhedran@gmail.com",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/elhedran/rxjs-dew-history/issues"
    },
    "homepage": "https://github.com/elhedran/rxjs-dew-history#readme",
    "devDependencies": {
        "@types/history": "^4.6.0",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.1",
        "shx": "^0.2.2",
        "tslint": "^5.4.3",
        "typescript": "2.3.4"
    },
    "dependencies": {
        "history": "^4.6.3",
        "rxjs": "^5.4.2",
        "rxjs-dew": "^0.6.0"
    },
    "peerDependencies": {
        "@types/history": "^4.6.0"
    }
